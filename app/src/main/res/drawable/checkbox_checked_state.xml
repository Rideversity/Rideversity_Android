<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimary"/>
            <size android:width="@dimen/twenty_dp" android:height="@dimen/twenty_dp"/>
            <stroke android:color="#8fb5db" android:width="@dimen/one_dp"/>
            <corners android:radius="@dimen/two_dp"/>
        </shape>
    </item>
    <item android:left="@dimen/two_dp" android:top="@dimen/two_dp" android:right="@dimen/two_dp" android:bottom="@dimen/two_dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimary"/>
            <stroke android:color="#778899" android:width="@dimen/one_dp"/>
            <corners android:radius="@dimen/two_dp"/>
        </shape>
    </item>
    <item android:drawable="@drawable/check_img"
         android:width="@dimen/ten_dp"
        android:height="@dimen/ten_dp"
        android:gravity="center"
        />
</layer-list>
